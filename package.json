  },
  "bugs": {
    "url": "https://github.com/kei-g/bigint-fraction.js/issues"
  },
  "categories": [
    "Data Science"
  ],
  "description": "Fraction composed of bigint",
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "jsdoc": "^3.6.7",
    "mkdirp": "^1.0.4",
    "mocha": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "terser": "^5.7.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": ">=14.17.5"
  },
  "files": [
    "CHANGELOG.md",
    "index.d.ts",
    "index.js",
    "lib"
  ],
  "homepage": "https://github.com/kei-g/bigint-fraction.js",
  "keywords": [
    "bigfraction",
    "bigint",
    "fraction"
  ],
  "license": "BSD-3-Clause",
  "main": "index.js",
  "name": "bigint-fraction",
  "nyc": {
    "branches": 100,
    "funcs": 100,
    "lines": 100,
    "statements": 100,
    "watermarks": {
      "branches": [80, 95],
      "functions": [80, 95],
      "lines": [80, 95],
      "statements": [80, 95]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kei-g/bigint-fraction.js.git"
  },
  "scripts": {
    "build": "npm-run-all -p clean lint -p build:mkdir build:tsc -p build:doc terse:*",
    "build:doc": "jsdoc build/lib/*.js",
    "build:mkdir": "mkdirp lib",
    "build:tsc": "tsc",
    "clean": "rimraf build/ index.d.ts index.js lib/ out/",
    "cover": "nyc --check-coverage -r html -r text _mocha",
    "lint": "eslint src/*.ts src/lib/*.ts",
    "postpublish": "run-s clean",
    "prepublishOnly": "run-s build",
    "start": "ts-node src/index.ts",
    "terse:euclidian": "terser build/lib/euclidian.js -c -m -o lib/euclidian.js --toplevel",
    "terse:fraction": "terser build/lib/fraction.js -c -m -o lib/fraction.js --toplevel",
    "terse:index": "terser build/index.js -c -m -o index.js --toplevel",
    "test": "run-p cover lint"
  },
  "version": "1.0.2"
}
